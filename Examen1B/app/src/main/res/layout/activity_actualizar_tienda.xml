<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ActualizarTienda">

    <EditText
            android:layout_width="282dp"
            android:layout_height="46dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txtDireccion"
            app:layout_constraintEnd_toEndOf="@+id/txtNombres" android:layout_marginEnd="20dp"
            android:layout_marginRight="20dp" app:layout_constraintStart_toStartOf="@+id/txtNombres"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginBottom="4dp"
            app:layout_constraintBottom_toTopOf="@+id/txtFecha"/>
    <TextView
            android:text="RUC"
            android:layout_width="72dp"
            android:layout_height="49dp"
            android:id="@+id/textView7" app:layout_constraintStart_toStartOf="@+id/textView3"
            app:layout_constraintEnd_toEndOf="@+id/textView3" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/textView6" app:layout_constraintHorizontal_bias="1.0"/>
    <TextView
            android:text="MATRIZ"
            android:layout_width="72dp"
            android:layout_height="49dp"
            android:id="@+id/textView6" app:layout_constraintEnd_toEndOf="@+id/textView7"
            app:layout_constraintStart_toStartOf="@+id/textView7" app:layout_constraintHorizontal_bias="1.0"
            android:layout_marginBottom="308dp" app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
            android:text="Direccion"
            android:layout_width="72dp"
            android:layout_height="49dp"
            android:id="@+id/textView5" app:layout_constraintStart_toStartOf="@+id/textView4"
            app:layout_constraintEnd_toEndOf="@+id/textView4"
            app:layout_constraintBottom_toTopOf="@+id/textView3" app:layout_constraintHorizontal_bias="1.0"/>
    <EditText
            android:layout_width="281dp"
            android:layout_height="45dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txtNombres" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/txtDireccion" app:layout_constraintVertical_bias="1.0"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp"
            android:layout_marginStart="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" app:layout_constraintHorizontal_bias="0.929"/>
    <TextView
            android:text="Nombre"
            android:layout_width="72dp"
            android:layout_height="49dp"
            android:id="@+id/textView4" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/textView5" app:layout_constraintVertical_bias="1.0"
            android:layout_marginEnd="28dp" app:layout_constraintEnd_toStartOf="@+id/txtNombres"
            android:layout_marginRight="28dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="1.0"/>
    <EditText
            android:layout_width="280dp"
            android:layout_height="45dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txtMatriz"
            app:layout_constraintEnd_toEndOf="@+id/txtRuc"
            app:layout_constraintStart_toStartOf="@+id/txtRuc" app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginBottom="312dp" app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
            android:text="Fecha Apertura"
            android:layout_width="72dp"
            android:layout_height="49dp"
            android:id="@+id/textView3" app:layout_constraintStart_toStartOf="@+id/textView5"
            app:layout_constraintEnd_toEndOf="@+id/textView5"
            app:layout_constraintBottom_toTopOf="@+id/textView7" app:layout_constraintHorizontal_bias="1.0"/>
    <EditText
            android:layout_width="282dp"
            android:layout_height="47dp"
            android:inputType="date"
            android:ems="10"
            android:id="@+id/txtFecha"
            app:layout_constraintEnd_toEndOf="@+id/txtDireccion"
            app:layout_constraintStart_toStartOf="@+id/txtDireccion"
            app:layout_constraintBottom_toTopOf="@+id/txtRuc" app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginBottom="4dp"/>
    <EditText
            android:layout_width="282dp"
            android:layout_height="48dp"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/txtRuc"
            app:layout_constraintStart_toStartOf="@+id/txtFecha"
            app:layout_constraintEnd_toEndOf="@+id/txtFecha" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/txtMatriz" app:layout_constraintHorizontal_bias="0.0"/>
    <Button
            android:text="Actualizar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/txtMatriz" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="0.567"
            app:layout_constraintVertical_bias="0.459"/>
</android.support.constraint.ConstraintLayout>