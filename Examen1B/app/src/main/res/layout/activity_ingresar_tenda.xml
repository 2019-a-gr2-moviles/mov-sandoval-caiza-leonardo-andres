<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".IngresarTenda">

    <Button
            android:text="CANCELAR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnCancelar"
            android:layout_marginTop="5dp" app:layout_constraintTop_toBottomOf="@+id/btnAceptar"
            app:layout_constraintStart_toStartOf="@+id/btnAceptar" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="@+id/btnAceptar"
            app:layout_constraintHorizontal_bias="0.501"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.122"/>
    <TextView
            android:text="RUC"
            android:layout_width="72dp"
            android:layout_height="49dp"
            android:id="@+id/textView11"
            tools:layout_editor_absoluteY="223dp" app:layout_constraintStart_toStartOf="@+id/textView10"
            app:layout_constraintEnd_toEndOf="@+id/textView10" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.0"/>
    <EditText
            android:layout_width="282dp"
            android:layout_height="48dp"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/txtRuc2"
            app:layout_constraintStart_toStartOf="@+id/txtFecha2"
            app:layout_constraintEnd_toEndOf="@+id/txtFecha2" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintTop_toBottomOf="@+id/txtFecha2" android:layout_marginTop="4dp"/>
    <EditText
            android:layout_width="280dp"
            android:layout_height="45dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txtMatriz2"
            app:layout_constraintEnd_toEndOf="@+id/txtRuc2"
            app:layout_constraintStart_toStartOf="@+id/txtRuc2" app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/txtRuc2"
            app:layout_constraintVertical_bias="0.0"/>
    <TextView
            android:text="MATRIZ"
            android:layout_width="72dp"
            android:layout_height="49dp"
            android:id="@+id/textView12"
            app:layout_constraintEnd_toEndOf="@+id/textView11"
            app:layout_constraintStart_toStartOf="@+id/textView11" app:layout_constraintHorizontal_bias="1.0"
            android:layout_marginBottom="408dp" app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
            android:text="Fecha Apertura"
            android:layout_width="72dp"
            android:layout_height="49dp"
            android:id="@+id/textView10"
            tools:layout_editor_absoluteY="174dp"
            app:layout_constraintEnd_toEndOf="@+id/textView9" app:layout_constraintStart_toStartOf="@+id/textView9"
            app:layout_constraintHorizontal_bias="0.0"/>
    <EditText
            android:layout_width="282dp"
            android:layout_height="47dp"
            android:inputType="date"
            android:ems="10"
            android:id="@+id/txtFecha2"
            app:layout_constraintEnd_toEndOf="@+id/txtDireccion2"
            app:layout_constraintStart_toStartOf="@+id/txtDireccion2" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintTop_toBottomOf="@+id/txtDireccion2"/>
    <TextView
            android:text="Direccion"
            android:layout_width="72dp"
            android:layout_height="49dp"
            android:id="@+id/textView9" app:layout_constraintStart_toStartOf="@+id/textView8"
            app:layout_constraintEnd_toEndOf="@+id/textView4"
            app:layout_constraintBottom_toTopOf="@+id/textView3" app:layout_constraintHorizontal_bias="1.0"
            android:layout_marginTop="48dp"
            app:layout_constraintTop_toTopOf="@+id/textView8"
    />
    <EditText
            android:layout_width="282dp"
            android:layout_height="46dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txtDireccion2"
            app:layout_constraintEnd_toEndOf="@+id/txtNombres2"
            app:layout_constraintStart_toStartOf="@+id/txtNombres2"
            app:layout_constraintHorizontal_bias="0.0" android:layout_marginBottom="4dp"
            app:layout_constraintBottom_toTopOf="@+id/txtFecha"
            app:layout_constraintTop_toBottomOf="@+id/txtNombres2" android:layout_marginTop="4dp"/>
    <TextView
            android:text="Nombre"
            android:layout_width="72dp"
            android:layout_height="49dp"
            android:id="@+id/textView8" android:layout_marginTop="76dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/textView5" app:layout_constraintVertical_bias="1.0"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toStartOf="@+id/txtNombres"
            android:layout_marginRight="8dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="44dp" android:layout_marginStart="44dp"
            app:layout_constraintHorizontal_bias="1.0"/>
    <EditText
            android:layout_width="278dp"
            android:layout_height="48dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/txtNombres2" android:layout_marginTop="76dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/txtDireccion" app:layout_constraintVertical_bias="1.0"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginRight="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/textView8"/>
    <Button
            android:text="ACEPTAR"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/btnAceptar"
            android:layout_marginTop="80dp" app:layout_constraintTop_toBottomOf="@+id/txtMatriz2"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.527"/>
</android.support.constraint.ConstraintLayout>